<html>
	<%- include('./head.ejs') %>
	<body>
		<%- include('./navigation.ejs') %>
		<div>
			<form action='/blogs/edit/<%= blog._id %>' method='POST'>
				<label>Title </label>
				<input type='text' name='title' value= <%= blog.title %> >
				
				<label>Snippet </label>
				<input type='text' name='snippet' value= <%= blog.snippet %> >
				
